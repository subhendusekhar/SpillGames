package src.scenes {		import flash.display.MovieClip;	import src.game.SurgeryStep;	import src.game.GameLogic;			public class LastFrameIntro extends SurgeryStep {				var m_counter:int = 5;				public function LastFrameIntro() {			scheduleForUpdate();		}		override public function update()		{			m_counter--;			if(m_counter < 0)			{				GameLogic.sharedGameLogic().showSceneByName("src.scenes.MainMenu");				unscheduleForUpdate();			}		}	}	}