package src.scenes {		import flash.display.MovieClip;	import src.game.GameLogic;	import src.game.SurgeryStep;			public class MC_05 extends SurgeryStep {				private var m_counter:int = 60;				public function MC_05() {			scheduleForUpdate();		}				override public function update()		{			if(GameLogic.sharedGameLogic().isHardMode() && --m_counter <= 0)			{				GameLogic.sharedGameLogic().showSceneByName(getProperties().nextSceneName);				unscheduleForUpdate();			}		}	}	}