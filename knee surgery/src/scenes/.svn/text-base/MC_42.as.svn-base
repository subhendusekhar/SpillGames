package src.scenes {		import flash.display.MovieClip;	import src.game.SurgeryStep;			public class MC_42 extends SurgeryStep {						public function MC_42() {			stop();			scheduleForUpdate();		}				override public function update()		{			if(slider5.reachedXPortion >= 41)			{				unscheduleForUpdate();				slider5.unscheduleForUpdate();				this.removeChild(slider5);				this.play();			}		}	}	}