package src.scenes {		import flash.display.MovieClip;	import src.game.GameLogic;	import src.game.SurgeryStep;	import src.GLP.GMGDebugger;		public class MC_Warning extends SurgeryStep {				private var m_counter:Number = 0;				public function MC_Warning() {			scheduleForUpdate();		}				override public function update()		{			m_counter++;			if(m_counter == 90)			{				GameLogic.sharedGameLogic().showSceneByName(getProperties().nextSceneName);			}		}	}	}