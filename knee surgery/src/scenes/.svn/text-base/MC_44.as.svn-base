package src.scenes {		import flash.display.MovieClip;	import src.game.SurgeryStep;			public class MC_44 extends SurgeryStep {						public function MC_44() {			stop();			scheduleForUpdate();		}				override public function update()		{			if(slider6.reachedXPortion >= 42)			{				unscheduleForUpdate();				slider6.unscheduleForUpdate();				this.removeChild(slider6);				this.play();			}		}	}	}