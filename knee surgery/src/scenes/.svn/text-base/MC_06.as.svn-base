package src.scenes {		import flash.display.MovieClip;	import src.game.GameLogic;	import src.game.SurgeryStep;			public class MC_06 extends SurgeryStep {				private var counter:int = 60;				public function MC_06() {			if(GameLogic.sharedGameLogic().isHardMode())			{				stop();				scheduleForUpdate();			}		}				override public function update()		{			counter--;			if(counter == 0)			{				unscheduleForUpdate();				play();			}		}	}	}