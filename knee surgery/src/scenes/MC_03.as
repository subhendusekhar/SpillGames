package src.scenes {		import flash.display.MovieClip;	import src.game.SurgeryStep;	import flash.events.MouseEvent;		public class MC_03 extends SurgeryStep {						public function MC_03() {			this.addEventListener(MouseEvent.MOUSE_MOVE, moveEv);		}				private function moveEv(ev:MouseEvent)		{			if(this.mouseY < 300 && this.mouseY > 100)			{				this.camera.y = this.mouseY;			}		}	}	}