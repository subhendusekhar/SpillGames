package src.scenes {		import flash.display.MovieClip;	import src.game.SurgeryStep;			public class MC_13 extends SurgeryStep {						public function MC_13() {			stop();			scheduleForUpdate();		}				override public function update()		{			if(slider1.reachedXPortion >= 31)			{				unscheduleForUpdate();				slider1.unscheduleForUpdate();				this.removeChild(slider1);				this.play();			}		}	}	}