package src.scenes {		import flash.display.MovieClip;	import src.game.SurgeryStep;			public class MC_17 extends SurgeryStep {						public function MC_17() {			stop();			scheduleForUpdate();		}				override public function update()		{			if(slider3.reachedXPortion >= 31)			{				unscheduleForUpdate();				slider3.unscheduleForUpdate();				this.removeChild(slider3);				this.play();			}		}	}	}