package {		import flash.display.MovieClip;	import src.GLP.GLPScene;	import src.GLP.GLPDirector;	import src.game.GameLogic;	import flash.utils.getDefinitionByName;	import flash.events.MouseEvent;	import flash.utils.getQualifiedClassName;	import src.GLP.GMGDebugger;			public class Debugger extends GLPScene {				public function Debugger() 		{			this.visible = GMGDebugger.isDebugging();			if(GMGDebugger.isDebugging())			{				scheduleForUpdate();				goTo.addEventListener(MouseEvent.CLICK, goto);			}		}				override public function update()		{			if(GameLogic.sharedGameLogic().getCurrentScene() != null)			{				if(GameLogic.sharedGameLogic().getCurrentScene().toString() != mcName.text)				{					mcName.text = getQualifiedClassName(GameLogic.sharedGameLogic().getCurrentScene()).split("::")[1];				}			}		}				public function goto(ev:MouseEvent)		{			GameLogic.sharedGameLogic().showSceneByName(sceneName.text);		}	}	}