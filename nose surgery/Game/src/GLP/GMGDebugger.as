package src.GLP{	import flash.system.System;	import src.game.GameLogic;	import com.demonsters.debugger.MonsterDebugger;	public class GMGDebugger	{		private static var m_debugMode:Boolean = false;				public function GMGDebugger()		{		}				public static function activateDebugMode(activate:Boolean)		{			m_debugMode = activate;		}				public static function isDebugging():Boolean		{			return m_debugMode;		}				public static function traceString(string:String)		{			if(m_debugMode)			{				//var tmp:Array = new Error().getStackTrace().split("\n")[2].split(" ")[1].split("/");				//trace(tmp[0] + "-" + tmp[1] + ": " + string);				MonsterDebugger.trace(null, string);				trace(string);			}		}	}}