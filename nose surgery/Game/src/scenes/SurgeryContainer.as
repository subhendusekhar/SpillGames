package src.scenes {		import flash.display.MovieClip;	import src.GLP.GLPScene;	import src.GLP.GLPDirector;	import src.GLP.AnimationList;	import src.game.GameLogic;	import src.game.SoundManager;	import src.game.PlayerData;	import flash.net.SharedObject;	import src.GLP.GMGDebugger;	import src.GLP.Analytics;	import src.elements.SoundControl;		public class SurgeryContainer extends GLPScene {				public function SurgeryContainer() 		{			GameLogic.sharedGameLogic().setContainer(m_mainContent);			GameLogic.sharedGameLogic().showSceneByName("src.scenes.LastFrameIntro");			scheduleForUpdate();		}				public function getSoundControl():SoundControl		{			return getChildByName("soundControl") as SoundControl;		}				override public function update()		{			GameLogic.sharedGameLogic().update();			var sc:SoundControl = getChildByName("soundControl") as SoundControl;			sc.updateStatus();			if(GameLogic.sharedGameLogic().getCurrentSceneName().indexOf("MainMenu") > 0 			   || GameLogic.sharedGameLogic().getCurrentSceneName().indexOf("LastFrameIntro") > 0)			{				sc.visible = false;			}			else			{				sc.visible = true;			}						var sl:SpilLogo = getChildByName("spilLogo") as SpilLogo;			if(GameLogic.sharedGameLogic().getCurrentSceneName().indexOf("MC_") < 0			   && GameLogic.sharedGameLogic().getCurrentSceneName().indexOf("MCC_") < 0)			{				sl.visible = false;			}			else			{				sl.visible = true;			}		}	}	}