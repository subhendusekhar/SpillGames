package src.scenes {		import flash.display.MovieClip;	import src.game.SurgeryStep;	import flash.events.MouseEvent;	import src.game.SpilAPIManager;	import src.game.PlayerData;	import src.game.GameLogic;	import flash.text.TextField;		public class GameOverMenu extends SurgeryStep {				public function GameOverMenu() {			moreGames.addEventListener(MouseEvent.CLICK, moreGamesClick);			playAgain.addEventListener(MouseEvent.CLICK, playAgainClick);						SpilAPIManager.sharedSpilAPI().localizeTextField(failed);			SpilAPIManager.sharedSpilAPI().localizeTextField(badJob);		}				private function moreGamesClick(ev:MouseEvent)		{			SpilAPIManager.sharedSpilAPI().navigateToMoreGames();		}				private function playAgainClick(ev:MouseEvent)		{			GameLogic.sharedGameLogic().restart();			if(PlayerData.sharedData().getTimesCompleted() > 0)			{				GameLogic.sharedGameLogic().showSceneByName("src.scenes.DifficultyMenu");			}			else			{				GameLogic.sharedGameLogic().startGame(false, PlayerData.sharedData().getNoBlood());			}		}	}	}