package src.scenes {		import flash.display.MovieClip;	import src.game.SurgeryStep;	import src.game.GameLogic;			public class MC_Adrenaline5 extends SurgeryStep {				var m_count:int = 2;				public function MC_Adrenaline5()		{			stop();			scheduleForUpdate();			GameLogic.sharedGameLogic().revive();			if(GameLogic.sharedGameLogic().getOverByTime())			{				GameLogic.sharedGameLogic().addTime(1, 0);			}		}				override public function update()		{			m_count--;			if(m_count < 0)			{				GameLogic.sharedGameLogic().showSceneByName(GameLogic.sharedGameLogic().getReviveSceneName(), true);			}		}	}	}